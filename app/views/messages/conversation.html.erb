<div class="page-header">
  <h1>My Conversations</h1>
</div>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Sender</th>
      <th>Message</th>

    </tr>
  </thead>

  <tbody>
    <% @messages.each do |message| %>
      <tr>
        <td><%= message.user.full_name %></td>
        <td><%= message.message %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

